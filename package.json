{
  "name": "cap-rfc",
  "version": "1.0.0",
  "description": "A simple CAP project.",
  "repository": "https://github.com/gregorwolf/cap-rfc.git",
  "license": "UNLICENSED",
  "private": true,
  "engines": {
    "node": "^22"
  },
  "dependencies": {
    "@sap-rfc/node-rfc-library": "^1.1.0",
    "@sap/cds": "^9",
    "@sap/cds-rfc": "^2",
    "@sap/xssec": "^4",
    "dotenv": "^17.2.1",
    "express": "^4"
  },
  "devDependencies": {
    "@cap-js/cds-typer": "^0.34.0",
    "@cap-js/cds-types": "^0.10.0",
    "@cap-js/sqlite": "^2",
    "@sap/cds-dk": "^9"
  },
  "scripts": {
    "start": "cds-serve"
  },
  "cds": {
    "log": {
      "format": "plain"
    },
    "requires": {
      "SAP_ABAP_BACKEND_RFC": {
        "kind": "rfc",
        "model": "srv/external/SAP_ABAP_BACKEND_RFC",
        "[production]": {
          "credentials": {
            "destination": "SAP_ABAP_BACKEND_RFC"
          }
        }
      },
      "[production]": {
        "auth": "xsuaa"
      },
      "S4H": {
        "kind": "rfc",
        "model": "srv/external/S4H",
        "[production]": {
          "credentials": {
            "destination": "S4H"
          }
        }
      }
    }
  }
}
